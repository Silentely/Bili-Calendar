{
  "name": "bili-calendar",
  "version": "1.0.0",
  "description": "将B站追番列表转换为日历订阅，支持 iCal/ICS 格式",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development node --watch server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "build": "cp -r netlify_functions/* netlify/functions-build/ && cp main.js netlify/functions-build/"
  },
  "keywords": [
    "bilibili",
    "calendar",
    "ics",
    "anime",
    "bili"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "express": "^4.18.2",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/runtime": "^7.24.3",
    "@netlify/functions": "^2.0.0",
    "babel-loader": "^9.1.3",
    "netlify-lambda": "^2.0.16",
    "npm-force-resolutions": "^0.0.10",
    "webpack": "^5.90.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "braces": "3.0.3",
    "**/braces": "3.0.3"
  }
}