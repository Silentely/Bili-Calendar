{
  "name": "bili-calendar",
  "version": "1.1.7",
  "description": "将B站追番列表转换为日历订阅，支持 iCal/ICS 格式",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development node --watch server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "build": "node scripts/update-readme-year.js && node scripts/build-netlify.mjs",
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "test": "node --test"
  },
  "keywords": [
    "bilibili",
    "calendar",
    "ics",
    "anime",
    "bili"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.0",
    "compression": "^1.8.1",
    "express": "^4.18.2",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "eslint": "^9.32.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "braces": "3.0.3",
    "**/braces": "3.0.3"
  }
}
